.top {
	margin-top:75px;
}

/* Margins between new blocks of content */

.block-margin {
	margin:75px 0px;
}

/* Lead or introductory paragraph text */

.lead {
	font-size:19px;
	font-weight:100;
}

.last {
	margin-bottom:75px;
}

.vr-page .lead.last {
	margin-bottom:0px;
}


/* Centers a div in a column */
/* centered columns styles */
.row-centered {
	text-align:center;
}
.col-centered {
	display:inline-block;
	float:none;
	/* reset the text-align */
	text-align:left;
	/* inline-block space fix */
	margin-right:-4px;
}



